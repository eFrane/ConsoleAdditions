{
  "name": "efrane/console-additions",
  "description": "Helper Classes for Symfony Console",
  "type": "library",
  "keywords": [
    "symfony", "console", "tools", "additions", "file output",
    "multiplexed output"
  ],
  "require": {
    "php": ">7.4,<=8.2",
    "symfony/console": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "league/flysystem": "^2.0 || ^3.0",
    "symfony/process": "^6.0",
    "phpstan/phpstan": "^1.0.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Stefan Graupner",
      "email": "stefan.graupner@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "EFrane\\ConsoleAdditions\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "suggest": {
    "league/flysystem": "To use the FlysystemFileOutput",
    "symfony/process": "To add shell commands to Batch"
  }
}
