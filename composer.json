{
  "name": "efrane/console-additions",
  "description": "Helper Classes for Symfony Console",
  "type": "library",
  "require": {
    "symfony/console": "^2.7|^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "league/flysystem": "^1.0",
    "symfony/filesystem": "^2.7|^3.3"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Stefan Graupner",
      "email": "stefan.graupner@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "EFrane\\ConsoleAdditions\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "suggest": {
    "league/flysystem": "To use the FlysystemFileOutput (NYI)",
    "symfony/filesystem": "To use the SymfonyFileOutput (NYI)"
  }
}
